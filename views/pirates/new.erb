<h1>The Pirate Form</h1>
<form action="/show" method="post">
  Pirate Name: <input type="text" name="pirate[name]">
  <br/>
  Pirate Weight: <input type="text" name="pirate[weight]">
  <br/>
  Pirate Height: <input type="text" name="pirate[height]">
  <br/>
  Ship Name: <input type="text" name="ship[courses][][topic]">
  <br/>
  Ship Type: <input type="text" name="ship[courses][][name]">
  <br/>
  Ship Booty: <input type="text" name="ship[courses][][topic]">
  <input type="submit">
</form>
